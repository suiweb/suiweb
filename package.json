{
    "name": "suiweb",
    "version": "1.0.0-beta.1",
    "scripts": {
        "dev": "vite",
        "build": "npm run build-dist && npm run build-js",
        "build-dist": "vite build",
        "build-js": "rm -rf ./dist-js/* && tsc --project tsconfig.emit.json && tsc-esm --project tsconfig.emit.json",
        "lint": "eslint ./src --ext .ts,.tsx --fix --ignore-path .gitignore --max-warnings 0",
        "type-check": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git@github.zhaw.ch:pa-schuhsim-siegetim/suiweb.git"
    },
    "license": "ISC",
    "devDependencies": {
        "@digitak/tsc-esm": "^3.1.4",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "typescript": "^4.8.3",
        "vite": "^3.1.4",
        "vite-plugin-babel": "^1.1.2",
        "vite-plugin-dts": "^1.7.0"
    }
}
